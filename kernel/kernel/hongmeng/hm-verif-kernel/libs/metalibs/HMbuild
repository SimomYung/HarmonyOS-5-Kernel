subdir-CONFIG_ARM: arch/arm

subdir-y: libalgo
subdir-y: include

lib-static-y: metalibs_static
output-metalibs_static: metalibs
install-metalibs_static-y: LIBDIR

lib-static-y: metalibs_fpic_static
