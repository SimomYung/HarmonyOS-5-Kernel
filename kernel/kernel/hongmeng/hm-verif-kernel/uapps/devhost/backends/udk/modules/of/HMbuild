include-of.proxy-y: ../../include

cflags-of.proxy-y: -DUDK_MODULE

src-of.proxy-y: of_fdt.c
src-of.proxy-y: of_dev.c
src-of.proxy-y: of_proxy.c

linklib-of.proxy-y: -Wl,--exclude-libs=ALL
linklib-of.proxy-y: -Wl,--gc-sections
linklib-of.proxy-y: Libfdt::Libfdt_static
