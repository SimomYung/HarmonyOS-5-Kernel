lib-dynamic-y: of_udk

findpkg: Libfdt

include-of_udk-y: ../../include

cflags-of_udk-y: -DUDK_MODULE

src-of_udk-y: of_udk.c
src-of_udk-y: of_fdt.c
src-of_udk-y: of_dev.c
src-of_udk-y: spec_dev.c

linklib-of_udk-y: -Wl,--exclude-libs=ALL
linklib-of_udk-y: -Wl,--gc-sections
linklib-of_udk-y: Libfdt::Libfdt_static

# of.proxy.udk
