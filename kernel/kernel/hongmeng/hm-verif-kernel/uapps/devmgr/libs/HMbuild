subdir-y: libdevhost
subdir-y: libhmsrv_io

cflags-.-CONFIG_NO_COMPAT: -D__sysif_server_devhost_no_compat32_handlers__
cflags-.-CONFIG_NO_ILP32: -D__sysif_server_devhost_no_ilp32_handlers__
cflags-.-CONFIG_ARM: -D__HM_ARM_ARCH__=7

include-.-y: include

installinc: include/libhmsrv_io
installinc: include/libdevhost

installfindmodule: FindHMDevmgrLibs.cmake
installfindmodule: FindHMDevmgrLibs_sysk.cmake
