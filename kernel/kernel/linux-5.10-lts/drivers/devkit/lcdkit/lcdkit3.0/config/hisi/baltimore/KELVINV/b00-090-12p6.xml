<hwlcd>
	<PanelEntry>
		<MipiDsiTimingSupport>1</MipiDsiTimingSupport>
		<!--Mipi Horizontal sync area -->
		<MipiHSyncArea>32</MipiHSyncArea>
		<!--Mipi Horizontal back porch -->
		<MipiHBackPorch>80</MipiHBackPorch>
		<!--Mipi Horizontal line time -->
		<MipiHLineTime>1782</MipiHLineTime>
		<!--Mipi Dpi horizontal size -->
		<MipiDpiHSize>1622</MipiDpiHSize>
		<!--Mipi Vertical sync area -->
		<MipiVSyncArea>10</MipiVSyncArea>
		<!--Mipi Vertical back porch -->
		<MipiVBackPorch>27</MipiVBackPorch>
		<!--Mipi Vertical front porch -->
		<MipiVFrontPorch>3</MipiVFrontPorch>
		<NonCheckLdiPorch>1</NonCheckLdiPorch>
		<MipiDsiVersion>0</MipiDsiVersion>
		<LcdKitPcSupport>1</LcdKitPcSupport>
		<!--Kirin 990 Mipi porch config end-->
		<!--Panel information config start-->
		<!--Panel type:8-video mode, 16-command mode-->
		<PanelCmdType>8</PanelCmdType>
		<MipiBurstMode>2</MipiBurstMode>
		<!--Pixel clock-->
		<PanelPxlClk>206</PanelPxlClk>
		<!--Pixel clock division ratio-->
		<PanelPxlClkDiv>1</PanelPxlClkDiv>
		<!--Ifbc type:0-none, 1-orise2x, 2-orise3x, 3-himax2x,
		4-rsp2x, 5-rsp3x, 6-vesa2x single, 7-vesa3x single
		8-vesa2x dual, 9-vesa3x dual, 10-vesa3.75x dual-->
		<PanelIfbcType>0</PanelIfbcType>
		<MipiColorMode>5</MipiColorMode>
		<!--DSS low power setting:0-close(video mode), 7-open(command mode)-->
		<PanelVsynCtrType>0</PanelVsynCtrType>
		<!--bl type:1-none, 2-pwm, 4-blpwm, 8-mipi, 16-sh blpwm-->
		<PanelBlType>4</PanelBlType>
		<!--bl ic type:1-reg only, 2-pwm only, 3-muti-ramp, 4-ramp-muti, 6-i2c only, 7-blpwm-cabc-->
		<PanelBlIcCtrlType>7</PanelBlIcCtrlType>
		<PanelBlPwmPreciType>2</PanelBlPwmPreciType>
		<!--bl min brightness setting-->
		<PanelBlmin>200</PanelBlmin>
		<!--bl max brightness setting-->
		<PanelBlmax>1023</PanelBlmax>
		<!--bl default brightness setting-->
		<PanelBlDef>1000</PanelBlDef>
		<!--bl fastboot brightness setting-->
		<PanelBlBoot>1000</PanelBlBoot>
		<!--Mipi lane number:c-phy:0-1port, 1-2port, 2-3port d-phy:0-1lane, 1-2lane, 2-3lane, 3-4lane-->
		<MipiLaneNums>3</MipiLaneNums>
		<!--mipi dsi support-->
		<MipiDsiUptSupport>0</MipiDsiUptSupport>
		<!--Mipi dsi default clock-->
		<MipiDsiBitClk>624</MipiDsiBitClk>
		<!--Mipi dynamic clk_1-->
		<MipiDsiBitClkValA>471</MipiDsiBitClkValA>
		<!--Mipi dynamic clk_2-->
		<MipiDsiBitClkValB>480</MipiDsiBitClkValB>
		<!--Mipi dynamic clk_3-->
		<MipiDsiBitClkValC>490</MipiDsiBitClkValC>
		<!--Mipi dynamic clk_4-->
		<MipiDsiBitClkValD>500</MipiDsiBitClkValD>
		<MipiDsiBitClkUpt>624</MipiDsiBitClkUpt>
		<MipiNonContinueEnable>1</MipiNonContinueEnable>
		<!--Mipi phy mode:0-D-phy, 1-C-phy-->
		<MipiPhyMode>0</MipiPhyMode>
		<!--Mipi clk post adjust-->
		<MipiClkPostAdjust>16</MipiClkPostAdjust>
		<MipiMaxTxEscClk>10</MipiMaxTxEscClk>
		<!--Power off performance optimize-->
		<PwrKeyLPressPwrOffSupport>0</PwrKeyLPressPwrOffSupport>
		<PwrKeyLPressPwrOffFlag>1</PwrKeyLPressPwrOffFlag>
		<PwrKeyLPressEsdStatus>1</PwrKeyLPressEsdStatus>
		<PanelQuiPwrOffCommand>"0x29, 0x01, 0x00, 0x00, 0x23, 0x01, 0x03, 0x51, 0x00, 0x00
				0x29, 0x01, 0x00, 0x00, 0x23, 0x01, 0x01, 0x28
				0x29, 0x01, 0x00, 0x00, 0x78, 0x01, 0x01, 0x10"</PanelQuiPwrOffCommand>
		<PwrKeyLPressTime2>3000</PwrKeyLPressTime2>
		<!--Esd enable-->
		<PanelEsdSupport>0</PanelEsdSupport>
		<PanelFacEsdSupport>0</PanelFacEsdSupport>
		<EsdGpioDetectSupport>0</EsdGpioDetectSupport>
		<EsdGpioDetectNum>69</EsdGpioDetectNum>
		<EsdGpioNormalValue>0</EsdGpioNormalValue>
		<TpEsdGpioEvent>1</TpEsdGpioEvent>
		<!--event send
		event, data, delay
		event:0-NONE, 1-VCI, 2-IOVCC, 3-VSP, 4-VSN, 5-RESET, 6-MIPI, 7-TS early-event, 8-TS later-event, 9-vdd
		data:0-power off, 1-power on
		delay:ms/TS:0-no-sync 1-sync
		-->
		<!--Power on timming-->
		<PowerOnStage>"1, 1, 10
			3, 1, 10
			2, 1, 15
			99, 2, 0
			5, 1, 2
			9, 1, 0
			99, 1, 200
			4, 1, 10"</PowerOnStage>
		<LPOnStage>"6, 1, 0"</LPOnStage>
		<HSOnStage>"0, 0, 0"</HSOnStage>

		<!--Power off timming-->
		<HSOffStage>"10, 0, 20
			6, 0, 15"</HSOffStage>
		<LPOffStage>"0, 0, 0"</LPOffStage>
		<PowerOffStage>"4, 0, 50
			99, 0, 0
			9, 0, 2
			5, 0, 2
			99, 3, 0
			9, 0, 0
			2, 0, 10
			3, 0, 10
			1, 0, 0"</PowerOffStage>
		<!--Power timming config end-->
		<!--Oem info-->
		<OemInfoSupport>0</OemInfoSupport>
		<TpReadSnSupport>0</TpReadSnSupport>
		<OemBarcode2DSupport>0</OemBarcode2DSupport>
		<!--backlight order-->
		<BacklightOrder>0</BacklightOrder>
		<!--Power timming config start-->
		<!--event define
		mode, number, amplitude
		mode:0-none, 1-regulator, 2-gpio
		number:gpio num or ldo num
		amplitude:3100000-3.1v, 1850000-1.85v, 5500000-5.5v
		-->
		<LcdVci>"2, 15, 3300000"</LcdVci>
		<LcdIovcc>"2, 55, 1800000"</LcdIovcc>
		<LcdVdd>"2, 180, 0"</LcdVdd>
		<LcdVsp>"2, 102, 1200000"</LcdVsp>
		<LcdVsn>"2, 179, 0"</LcdVsn>
		<LcdReset>"2, 2, 0"</LcdReset>
		<LcdTe0>"0, 0, 0"</LcdTe0>
		<LcdPowerIC>"0, 0, 0"</LcdPowerIC>
		<LcdBacklight>"0, 0, 0"</LcdBacklight>
		<LcdAod>"2, 191, 0"</LcdAod>
		<!--event send-->
		<PcdCmdsSupport>0</PcdCmdsSupport>
		<!-- for mipi check reg on -->
		<PanelBacklightCommandState>0</PanelBacklightCommandState>
		<PanelCheckRegOnSupport>0</PanelCheckRegOnSupport>
		<PanelCheckRegOffSupport>0</PanelCheckRegOffSupport>
	</PanelEntry>
</hwlcd>
