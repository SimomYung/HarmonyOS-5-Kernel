<hwlcd>
	<PanelEntry>
		<!--/* 0:DSI0 1:DSI1 2:DSI2 */ -->
		<ConnectorId0>0</ConnectorId0>
		<!--/* Max connector number is 2 */ -->
		<ConnectorNum>1</ConnectorNum>
		<!--Ldi config start-->
		<MipiDsiTimingSupport>1</MipiDsiTimingSupport>
		<!--Mipi Horizontal sync area -->
		<MipiHSyncArea>4</MipiHSyncArea>
		<!--Mipi Horizontal back porch -->
		<MipiHBackPorch>8</MipiHBackPorch>
		<!--Mipi Horizontal line time -->
		<MipiHLineTime>342</MipiHLineTime>
		<!--Mipi Dpi horizontal size -->
		<MipiDpiHSize>322</MipiDpiHSize>
		<!--Mipi Vertical sync area -->
		<MipiVSyncArea>8</MipiVSyncArea>
		<!--Mipi Vertical back porch -->
		<MipiVBackPorch>132</MipiVBackPorch>
		<!--Mipi Vertical front porch -->
		<MipiVFrontPorch>106</MipiVFrontPorch>
		<!--Panel information config start-->
		<!--Panel type:8-video mode, 16-command mode-->
		<PanelCmdType>16</PanelCmdType>
		<!--Pixel clock-->
		<PanelPxlClk>192</PanelPxlClk>
		<!--Panel fps-->
		<PanelFps>120</PanelFps>
		<!--Pixel clock division ratio-->
		<PanelPxlClkDiv>1</PanelPxlClkDiv>
		<!--Ifbc type:0-none, 1-orise2x, 2-orise3x, 3-himax2x,
		4-rsp2x, 5-rsp3x, 6-vesa2x single, 7-vesa3x single
		8-vesa2x dual, 9-vesa3x dual, 10-vesa3.75x dual-->
		<PanelIfbcType>5</PanelIfbcType>
		<VesaSliceWidth>640</VesaSliceWidth>
		<VesaSliceHeight>16</VesaSliceHeight>
		<!--0-ver1.1, 1-ver1.2-->
		<VesaDscVersion>1</VesaDscVersion>
		<!--0-RGB, 1-YUV, 2-YUV420, 3-YUV422, 4-YUV-444-->
		<VesaDscFormat>0</VesaDscFormat>
		<!--bit per pixel-->
		<VesaDscBpp>8</VesaDscBpp>
		<!--bit per component-->
		<VesaDscBpc>10</VesaDscBpc>
		<!--block pred enable-->
		<VesaDscBlkPredEn>1</VesaDscBlkPredEn>
		<!--line buff depth-->
		<VesaDscLineBuffDepth>11</VesaDscLineBuffDepth>
		<!--GENERATE_RC_PARAMETERS:0-default, 1-calculate-->
		<VesaDscGenRcParams>1</VesaDscGenRcParams>
		<!--DSS low power setting:0-close(video mode), 7-open(command mode)-->
		<PanelVsynCtrType>7</PanelVsynCtrType>
		<!--Pt station test support-->
		<PanelPtTestSupport>1</PanelPtTestSupport>
		<!--check reg support-->
		<PanelCheckRegSupport>0</PanelCheckRegSupport>
		<!--Project id support-->
		<PanelProjectIdSupport>0</PanelProjectIdSupport>
		<!--sn code support-->
		<PanelSnCodeSupport>1</PanelSnCodeSupport>
		<!--sn code reprocess-->
		<PanelSnCodeReprocessSupport>1</PanelSnCodeReprocessSupport>
		<!--Oem info-->
		<OemInfoSupport>1</OemInfoSupport>
		<OemBarcode2DSupport>1</OemBarcode2DSupport>
		<!--Esd enable-->
		<PanelEsdSupport>0</PanelEsdSupport>
		<PanelFacEsdSupport>0</PanelFacEsdSupport>
		<!--bl type:1-none, 2-pwm, 4-blpwm, 8-mipi, 16-sh blpwm-->
		<PanelBlType>8</PanelBlType>
		<!--bl min brightness setting-->
		<PanelBlmin>12</PanelBlmin>
		<!--bl max brightness setting-->
		<PanelBlmax>16380</PanelBlmax>
		<!--bl default brightness setting-->
		<PanelBlDef>6420</PanelBlDef>
		<!--Mipi lane number:c-phy:0-1port, 1-2port, 2-3port d-phy:0-1lane, 1-2lane, 2-3lane, 3-4lane-->
		<MipiLaneNums>3</MipiLaneNums>
		<!--mipi dsi support-->
		<MipiDsiUptSupport>0</MipiDsiUptSupport>
		<!--Mipi dsi default clock-->
		<MipiDsiBitClk>528</MipiDsiBitClk>
		<!--Mipi phy mode:0-D-phy, 1-C-phy-->
		<MipiPhyMode>0</MipiPhyMode>
		<MipiClkPostAdjust>16</MipiClkPostAdjust>
		<MipiMaxTxEscClk>10</MipiMaxTxEscClk>
		<MipiDataTHsPrepareAdjust>18</MipiDataTHsPrepareAdjust>
		<!--backlight order-->
		<BacklightOrder>1</BacklightOrder>
		<!--backlight ext byte flag-->
		<PanelBlextFlag>1</PanelBlextFlag>
		<!--backlight offset-->
		<BacklightWriteOffset>0x01</BacklightWriteOffset>
		<StandbyCurrentDetSupport>0</StandbyCurrentDetSupport>
		<StandbyCurrentChannelId>12</StandbyCurrentChannelId>
		<FacCheckIovccId>"ldo4"</FacCheckIovccId>
 
		<!--Mipi color mode:0-16bit_1, 1-16bit_2, 2-16bit_3, 3-18bit_1, 4-18bit_2, 5-24bit_1, 6-24bit_2, 7-24bit_3-->
		<MipiColorMode>15</MipiColorMode>
		<!--sensorhub aod support-->
		<SensorhubAodSupport>1</SensorhubAodSupport>
		<!--sensorhub enter exit ulps-->
		<SensorhubUlps>1</SensorhubUlps>
		<!--AodBlLevel:0-two level, 1-three level, 2-four level-->
		<AodBlLevel>1</AodBlLevel>
		<!--AodNormalBlLevel:0-two level, 1-three level, 2-four level-->
		<AodNormalBlLevel>1</AodNormalBlLevel>
		<!--support change aod init mode on sensorhub-->
		<AodInitModeSupport>1</AodInitModeSupport>
		<!-- for dieid read  -->
		<LcdPanelDieidSupport>0</LcdPanelDieidSupport>
		<!-- fac mbist -->
		<LcdMbistDetectSupport>0</LcdMbistDetectSupport>
		<LcdMbistDetectThreshold>1</LcdMbistDetectThreshold>
		<!--checksum support-->
		<PanelChecksumSupport>0</PanelChecksumSupport>
		<ChecksumReadLength>16</ChecksumReadLength>
		<!-- fac check sum2 -->
		<LCDCheckSum2DetSupport>0</LCDCheckSum2DetSupport>
		<LCDCheckSum2DetThreshold>1</LCDCheckSum2DetThreshold>
		<LCDCheckSum2DetDelay>500</LCDCheckSum2DetDelay>
		<LCDCheckSum2DetMinuteIndex1>12</LCDCheckSum2DetMinuteIndex1>
		<LCDCheckSum2DetMinuteIndex2>12</LCDCheckSum2DetMinuteIndex2>
		<LCDCheckSum2DetMinuteIndex3>6</LCDCheckSum2DetMinuteIndex3>
		<LCDCheckSum2ModelSwitch>1</LCDCheckSum2ModelSwitch>
		<!-- PCD support -->
		<PcdCmdsSupport>1</PcdCmdsSupport>
		<!--Pcd reg-value compare mode:
		0: == NG, != ok
		1: >= NG, < ok
		-->
		<PcdCmdsSupportCompareMode>0</PcdCmdsSupportCompareMode>
		<!--horizontal/vertical line test-->
		<VtcLineSupport>0</VtcLineSupport>
		<HorLineSupport>0</HorLineSupport>
		<HorLineDuration>600</HorLineDuration>
		<!-- support async mipi transfer -->
		<AsyncMipiTransferSupport>0</AsyncMipiTransferSupport>
		<!-- ulps process : 1-execute, 0-not execute; exit ulps at power on and enter ulps at power off -->
		<PoweroffUlpsSupport>1</PoweroffUlpsSupport>
		<!--ddic exception mode-->
		<DdicExceptionSupport>0</DdicExceptionSupport>

		<!--event send
		event, data, delay
		event:0-NONE, 1-VCI, 2-IOVCC, 3-VSP, 4-VSN, 5-RESET, 6-MIPI init code, 7-TS early-event, 8-TS later-event, 9-vdd
		event:10-AOD,11-BIAS, 12-AOD_MIPI, 13-MIPI_SWITCH, 14-AVDD_MIPI, 15-2ND_POWER_OFF_TS, 16-BLOCK_TS
		event:17-PMIC_CTRL, 18-PMIC_RESET, 19-PMIC_INIT, 21-BOOST_ENABLE, 22-BOOST_CTRL, 23-BOOST_BYPASS
		data:0-power off, 1-power on
		delay:ms/TS:0-no-sync 1-sync
		-->
		<!--Power on timming-->
		<PowerOnStage>"2, 1, 0
			18, 1, 5
			17, 1, 2
			1, 1, 6"</PowerOnStage>
		<LPOnStage>"5, 1, 36
			7, 1, 0
			6, 1, 0	
			8, 1, 0"</LPOnStage>
		<HSOnStage>"0, 0, 0"</HSOnStage>

		<!--Power off timming-->
		<HSOffStage>"7, 0, 0
			6, 0, 0
			8, 0, 0"</HSOffStage>
		<LPOffStage>"0, 0, 0"</LPOffStage>
		<PowerOffStage>"5, 0, 1
			1, 0, 10
			17, 0, 5
			2, 0, 10
			18, 0, 10"</PowerOffStage>
		<!--Power timming config end-->
		<!--Tp color support-->
		<TpColorSupport>0</TpColorSupport>
		<!--Backlight max nit-->
		<!--Power timming config start-->
		<!--event define
		mode, number, amplitude
		mode:0-none, 1-regulator, 2-gpio
		number:gpio num or ldo num
		amplitude:3100000-3.1v, 1850000-1.85v, 5500000-5.5v
		-->
		<IOVccRegulator>ldo4</IOVccRegulator>
		<LcdVci>"2, 353, 0"</LcdVci>
		<LcdIovcc>"1, 4, 1850000"</LcdIovcc>
		<LcdVdd>"0, 0, 0"</LcdVdd>
		<LcdVsp>"0, 0, 0"</LcdVsp>
		<LcdVsn>"0, 0, 0"</LcdVsn>
		<LcdMipiSwitch>"0, 0, 0"</LcdMipiSwitch>
		<LcdReset>"2, 10, 0"</LcdReset>
		<LcdTe0>"2, 65, 0"</LcdTe0>
		<PanelTEIndex>0</PanelTEIndex>
		<LcdBacklight>"0, 0, 0"</LcdBacklight>
		<LcdPmicCtrl>"2, 74, 0"</LcdPmicCtrl>
		<LcdPmicReset>"2, 8, 0"</LcdPmicReset>
		<LcdPmicInit>"0, 0, 0"</LcdPmicInit>
		<!--event send-->
		<!--aod power ctrol by ap-->
		<AodPmicCtrlByAp>1</AodPmicCtrlByAp>
		<AodPmicConfig>"9, 4, 1850000, 0x14, 0x5E, 0x60"</AodPmicConfig>

		<!--ltpo begin-->
		<PanelFpsUpdtSupport>1</PanelFpsUpdtSupport>
		<LtpoAodSupport>0</LtpoAodSupport>
		<!--oled type:0-ltps, 1-ltpo1, 2-ltpo2-->
		<OledType>0</OledType>
		<!--ltpo version:0-ltpo1, 1-ltpo2-->
		<LtpoVersion>1</LtpoVersion>
		<!--dfr mode:0-CONSTANT, 1-LONG_V, 2-LONG_H, 3-TE_SKIP_BY_MCU, 4-TE_SKIP_BY_ACPU-->
		<DFRMode>0</DFRMode>
		<!--panel ddic type:0-rd, 1-xhs-->
		<PanelDDICType>1</PanelDDICType>
		<!--support te freq info-->
		<PanelTEFreqInfo>"360"</PanelTEFreqInfo>
		<!--support te mask info-->
		<PanelTEMaskInfo>"2"</PanelTEMaskInfo>
		<!--panel downframe dimming sequence-->
		<PanelDimmingSeq>"60, 30, 10, 5, 45, 30, 10, 5, 72, 36, 18, 9, 40, 20, 10, 5"</PanelDimmingSeq>
		<!--panel downframe dimming sequence repeat time-->
		<PanelRepeatNum>"1, 2, 6, 1, 1, 2, 6, 1, 1, 2, 6, 1, 1, 2, 6, 1"</PanelRepeatNum>
		<PanelDimmingGearSeq>"120, 0, 1, 3, 90, 4, 5, 7, 72, 8, 9, 11, 60, 0, 1, 3, 45, 4, 4, 4, 40, 12, 13, 15, 36, 9, 10, 11, 30, 1, 2, 3"</PanelDimmingGearSeq>
		<PanelDimmingGearLen>8</PanelDimmingGearLen>
		<!--panel downframe dimming sequence-->
		<PanelFullAodDimmingSeq>"30, 10, 1"</PanelFullAodDimmingSeq>
		<!--panel downframe dimming sequence repeat time-->
		<PanelFullAodRepeatNum>"2, 8, 1"</PanelFullAodRepeatNum>
		<!--panel downframe dimming sequence-->
		<PanelDimmingSeq90Hz>"45, 30, 10, 1"</PanelDimmingSeq90Hz>
		<!--panel downframe dimming sequence repeat time-->
		<PanelRepeatNum90Hz>"1, 2, 8, 1"</PanelRepeatNum90Hz>
		<!--support aod te freq info-->
		<PanelAodTEFreqInfo>"90, 360"</PanelAodTEFreqInfo>
		<!--support aod te mask info-->
		<PanelAodTEMaskInfo>"2, 2"</PanelAodTEMaskInfo>
		<!--panel aod downframe dimming sequence-->
		<PanelAodDimmingSeq>"30, 10, 30, 10"</PanelAodDimmingSeq>
		<!--panel aod downframe dimming sequence repeat time-->
		<PanelAodRepeatNum>"3, 8, 3, 5"</PanelAodRepeatNum>
		<PanelAodDimmingGearSeq>"30, 0, 0, 1, 30, 2, 2, 3"</PanelAodDimmingGearSeq>
		<PanelAodDimmingGearLen>2</PanelAodDimmingGearLen>
		<PanelFullAodDimmingGearLen>1</PanelFullAodDimmingGearLen>
		<PanelBlTo1Hz>773</PanelBlTo1Hz>
		<PanelAodBlTo1Hz>1</PanelAodBlTo1Hz>
		<PanelDefaultFps>120</PanelDefaultFps>
		<PanelSupportFpsList>"30, 36, 40, 45, 60, 72, 90, 120"</PanelSupportFpsList>
		<PanelFPS60CMD>"service call AGPService 102 i32 60 i32 3"</PanelFPS60CMD>
		<PanelFPS90CMD>"service call AGPService 102 i32 90 i32 3"</PanelFPS90CMD>
		<PanelFPS120CMD>"service call AGPService 102 i32 120 i32 3"</PanelFPS120CMD>
		<PanelFPS45CMD>"service call AGPService 102 i32 45 i32 3"</PanelFPS45CMD>
		<PanelFPS30CMD>"service call AGPService 102 i32 30 i32 3"</PanelFPS30CMD>
		<PanelFPS24CMD>"service call AGPService 102 i32 24 i32 3"</PanelFPS24CMD>
		<PanelFPS10CMD>"service call AGPService 102 i32 10 i32 3"</PanelFPS10CMD>
		<PanelFPS1CMD>"service call AGPService 102 i32 1 i32 3"</PanelFPS1CMD>

		<!--Panel version support-->
		<PanelVersionSupport>0</PanelVersionSupport>

		<!--oneside driver mode detect test-->
		<OnesideModeSupport>0</OnesideModeSupport>
		<!--color aod detect-->
		<ColorAodDetectSupport>0</ColorAodDetectSupport>
	</PanelEntry>
</hwlcd>