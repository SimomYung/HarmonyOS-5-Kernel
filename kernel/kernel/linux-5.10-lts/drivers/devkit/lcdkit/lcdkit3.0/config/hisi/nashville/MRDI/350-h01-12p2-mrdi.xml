<hwlcd>
	<PanelEntry>
		<!--/* 0:DSI0 1:DSI1 2:DSI2 */ -->
		<ConnectorId0>0</ConnectorId0>
		<!--/* Max connector number is 2 */ -->
		<ConnectorNum>1</ConnectorNum>
		<!--Ldi config start-->
		<MipiDsiTimingSupport>1</MipiDsiTimingSupport>
		<!--Mipi Horizontal sync area -->
		<MipiHSyncArea>10</MipiHSyncArea>
		<!--Mipi Horizontal back porch -->
		<MipiHBackPorch>12</MipiHBackPorch>
		<!--Mipi Horizontal line time -->
		<MipiHLineTime>878</MipiHLineTime>
		<!--Mipi Dpi horizontal size -->
		<MipiDpiHSize>702</MipiDpiHSize>
		<!--Mipi Vertical sync area -->
		<MipiVSyncArea>18</MipiVSyncArea>
		<!--Mipi Vertical back porch -->
		<MipiVBackPorch>70</MipiVBackPorch>
		<!--Mipi Vertical front porch -->
		<MipiVFrontPorch>42</MipiVFrontPorch>
		<!--Panel information config start-->
		<!--Panel type:8-video mode, 16-command mode-->
		<PanelCmdType>16</PanelCmdType>
		<!--Pixel clock-->
		<PanelPxlClk>192</PanelPxlClk>
		<!--Panel fps-->
		<PanelFps>120</PanelFps>
		<PanelTEIndex>0</PanelTEIndex>
		<!--Pixel clock division ratio-->
		<PanelPxlClkDiv>1</PanelPxlClkDiv>
		<!--Ifbc type:0-none, 1-orise2x, 2-orise3x, 3-himax2x,
		4-rsp2x, 5-rsp3x, 6-vesa2x single, 7-vesa3x single
		8-vesa2x dual, 9-vesa3x dual, 10-vesa3.75x dual-->
		<PanelIfbcType>5</PanelIfbcType>
		<VesaSliceWidth>1400</VesaSliceWidth>
		<VesaSliceHeight>20</VesaSliceHeight>
		<!--0-ver1.1, 1-ver1.2-->
		<VesaDscVersion>0</VesaDscVersion>
		<!--0-RGB, 1-YUV, 2-YUV420, 3-YUV422, 4-YUV-444-->
		<VesaDscFormat>0</VesaDscFormat>
		<!--bit per pixel-->
		<VesaDscBpp>8</VesaDscBpp>
		<!--bit per component-->
		<VesaDscBpc>10</VesaDscBpc>
		<!--block pred enable-->
		<VesaDscBlkPredEn>1</VesaDscBlkPredEn>
		<!--line buff depth-->
		<VesaDscLineBuffDepth>11</VesaDscLineBuffDepth>
		<!--GENERATE_RC_PARAMETERS:0-default, 1-calculate-->
		<VesaDscGenRcParams>1</VesaDscGenRcParams>
		<!--DSS low power setting:0-close(video mode), 7-open(command mode)-->
		<PanelVsynCtrType>0</PanelVsynCtrType>
		<!--Mipi color mode:0-16bit_1, 1-16bit_2, 2-16bit_3, 3-18bit_1, 4-18bit_2, 5-24bit_1, 6-24bit_2, 7-24bit_3-->
		<MipiColorMode>15</MipiColorMode>
		<!--Pt station test support-->
		<PanelPtTestSupport>0</PanelPtTestSupport>
		<!--check reg support-->
		<PanelCheckRegSupport>0</PanelCheckRegSupport>
		<!--Check panel power status test support-->
		<PanelCheckRegOnSupport>0</PanelCheckRegOnSupport>
		<PanelCheckRegOnSupportDsmReport>0</PanelCheckRegOnSupportDsmReport>
		<PanelCheckRegOffSupport>0</PanelCheckRegOffSupport>
		<PanelCheckRegOffSupportDsmReport>0</PanelCheckRegOffSupportDsmReport>
		<!--Project id support-->
		<PanelProjectIdSupport>0</PanelProjectIdSupport>
		<!--sn code support-->
		<PanelSnCodeSupport>1</PanelSnCodeSupport>
		<!--Oem info-->
		<OemInfoSupport>1</OemInfoSupport>
		<OemBarcode2DSupport>1</OemBarcode2DSupport>
		<!--Esd enable-->
		<PanelEsdSupport>1</PanelEsdSupport>
		<PanelFacEsdSupport>0</PanelFacEsdSupport>
		<EsdGpioDetectSupport>2</EsdGpioDetectSupport>
		<EsdGpioDetectNum>222</EsdGpioDetectNum>
		<EsdGpioNormalValue>1</EsdGpioNormalValue>
		<TpEsdGpioEvent>0</TpEsdGpioEvent>

		<!--bl type:1-none, 2-pwm, 4-blpwm, 8-mipi, 16-sh blpwm-->
		<PanelBlType>8</PanelBlType>
		<!--bl min brightness setting-->
		<PanelBlmin>4</PanelBlmin>
		<!--bl max brightness setting-->
		<PanelBlmax>16380</PanelBlmax>
		<!--bl default brightness setting-->
		<PanelBlDef>4540</PanelBlDef>
		<!--backlight order-->
		<BacklightOrder>1</BacklightOrder>
		<!--backlight ext byte flag-->
		<PanelBlextFlag>0</PanelBlextFlag>
		<!--backlight offset-->
		<BacklightWriteOffset>0x80</BacklightWriteOffset>

		<!--Mipi lane number:c-phy:0-1port, 1-2port, 2-3port d-phy:0-1lane, 1-2lane, 2-3lane, 3-4lane-->
		<MipiLaneNums>3</MipiLaneNums>
		<!--mipi dsi support-->
		<MipiDsiUptSupport>0</MipiDsiUptSupport>
		<!--Mipi dsi default clock-->
		<MipiDsiBitClk>845</MipiDsiBitClk>
		<!--Mipi phy mode:0-D-phy, 1-C-phy-->
		<MipiPhyMode>0</MipiPhyMode>
		<MipiClkPostAdjust>215</MipiClkPostAdjust>
		<MipiMaxTxEscClk>10</MipiMaxTxEscClk>

		<!--Panel version support-->
		<PanelVersionSupport>1</PanelVersionSupport>

		<!--sensorhub aod support-->
		<SensorhubAodSupport>1</SensorhubAodSupport>
		<LtpoAodSupport>1</LtpoAodSupport>
		<!--AodBlLevel:0-two level, 1-three level, 2-four level-->
		<AodBlLevel>1</AodBlLevel>
		<!--Power off performance optimize-->
		<PwrKeyLPressPwrOffSupport>1</PwrKeyLPressPwrOffSupport>
		<PwrKeyLPressPwrOffFlag>0</PwrKeyLPressPwrOffFlag>
		<PwrKeyLPressEsdStatus>1</PwrKeyLPressEsdStatus>
		<PanelQuiPwrOffCommand>"0x39, 0x01, 0x00, 0x00, 0x05, 0x01, 0x04, 0xFF, 0x00, 0x00, 0x00
			0x29, 0x01, 0x00, 0x00, 0x05, 0x01, 0x03, 0x51, 0x00, 0x80
			0x29, 0x01, 0x00, 0x00, 0x32, 0x01, 0x01, 0x28"</PanelQuiPwrOffCommand>
		<PwrKeyLPressTime2>3400</PwrKeyLPressTime2>
		<!--event send
		event, data, delay
		event:0-NONE, 1-VCI, 2-IOVCC, 3-VSP, 4-VSN, 5-RESET, 6-MIPI, 7-TS early-event, 8-TS later-event, 9-vdd
		data:0-power off, 1-power on
		delay:ms/TS:0-no-sync 1-sync
		-->
		<!--Power on timming-->
		<PowerOnStage>"107, 0, 0
			10, 1, 0
			2, 1, 0
			1, 1, 20
			5, 1, 150"</PowerOnStage>
		<LPOnStage>"7, 1, 0
			6, 1, 0
			24, 1, 0
			8, 1, 0"</LPOnStage>
		<HSOnStage>"0, 0, 0"</HSOnStage>

		<!--Power off timming-->
		<HSOffStage>"7, 0, 0
			8, 0, 0
			6, 0, 0"</HSOffStage>
		<LPOffStage>"0, 0, 0"</LPOffStage>
		<PowerOffStage>"5, 0, 33
			1, 0, 0
			2, 0, 0
			10, 0, 0"</PowerOffStage>
		<!--Power timming config end-->
		<!--Tp color support-->
		<TpColorSupport>1</TpColorSupport>
		<!--Backlight max nit-->
		<!--Power timming config start-->
		<!--event define
		mode, number, amplitude
		mode:0-none, 1-regulator, 2-gpio
		number:gpio num or ldo num
		amplitude:3100000-3.1v, 1850000-1.85v, 5500000-5.5v
		-->
		<IOVccRegulator>ldo4</IOVccRegulator>
		<LcdVci>"2, 9, 0"</LcdVci>
		<LcdIovcc>"1, 4, 1850000"</LcdIovcc>
		<LcdVdd>"2, 223, 0"</LcdVdd>
		<LcdAod>"2, 78, 0"</LcdAod>
		<LcdVsp>"0, 0, 0"</LcdVsp>
		<LcdVsn>"0, 0, 0"</LcdVsn>
		<LcdReset>"2, 79, 0"</LcdReset>
		<LcdMipiSwitch>"0, 0, 0"</LcdMipiSwitch>
		<LcdTe0>"2, 65, 0"</LcdTe0>
		<LcdBacklight>"0, 0, 0"</LcdBacklight>
		<LcdPmicBoostEn>"2, 38, 3250"</LcdPmicBoostEn>
		<LcdPmicBoostOutput>"2, 333, 1"</LcdPmicBoostOutput>
		<!--event send-->
		<!--aod power ctrol by ap-->
		<AodPmicCtrlByAp>1</AodPmicCtrlByAp>
		<AodPmicConfig>"9, 4, 1850000, 0x14, 0x5E, 0x60"</AodPmicConfig>

		<!--fps begin-->
		<PanelFpsUpdtSupport>1</PanelFpsUpdtSupport>
		<PanelDefaultFps>120</PanelDefaultFps>
		<PanelSupportFpsList>"120, 144, 60"</PanelSupportFpsList>
		<PanelFPS60CMD>"service call AGPService 102 i32 60 i32 3"</PanelFPS60CMD>
		<PanelFPS120CMD>"service call AGPService 102 i32 120 i32 3"</PanelFPS120CMD>
		<PanelFPS144CMD>"service call AGPService 102 i32 144 i32 3"</PanelFPS144CMD>
		<!-- for support safe frame info -->
		<PanelSafeFrameInfo>"120"</PanelSafeFrameInfo>
		<!-- for support safe frame info -->
		<PanelLpSafeFrameInfo>"30"</PanelLpSafeFrameInfo>
		<!--hsa->hbp->dpi_hsize->hline_time->vsa->vbp->vfp->mipi_clk-->
		<FpsTo60DsiTimming>"10, 12, 702, 878, 18, 70, 42, 845"</FpsTo60DsiTimming>
		<!--hsa->hbp->dpi_hsize->hline_time->vsa->vbp->vfp->mipi_clk-->
		<FpsTo120DsiTimming>"10, 12, 702, 878, 18, 70, 42, 845"</FpsTo120DsiTimming>
		<!--hsa->hbp->dpi_hsize->hline_time->vsa->vbp->vfp->mipi_clk-->
		<FpsTo144DsiTimming>"10, 12, 702, 734, 18, 70, 42, 845"</FpsTo144DsiTimming>
		<!--fps end-->
		<!--ltpo begin-->
		<!--oled type:0-ltps, 1-ltpo1, 2-ltpo2-->
		<OledType>1</OledType>
		<!--ltpo version:0-ltpo1, 1-ltpo2-->
		<LtpoVersion>1</LtpoVersion>
		<!--dfr mode:
		0-CONSTANT, 1-LONG_V, 2-LONG_H, 3-TE_SKIP_BY_MCU, 4-TE_SKIP_BY_ACPU
		5-LONG_VH, 6-LONH_TE_SKIP_BY_MCU
		-->
		<DFRMode>6</DFRMode>
		<!--panel ddic type:0-rd, 1-xhs-->
		<PanelDDICType>1</PanelDDICType>
		<!--support te freq info-->
		<PanelTEFreqInfo>"120, 144"</PanelTEFreqInfo>

		<!--support te mask info-->
		<PanelTEMaskInfo>"0, 0"</PanelTEMaskInfo>
		<!--support aod te freq info-->
		<PanelAodTEFreqInfo>"30, 120"</PanelAodTEFreqInfo>
		<!--support aod te mask info-->
		<PanelAodTEMaskInfo>"0, 0"</PanelAodTEMaskInfo>
		<!--panel downframe dimming sequence-->
		<PanelDimmingSeq>"120, 60, 144"</PanelDimmingSeq>
		<!--panel downframe dimming sequence repeat time-->
		<PanelRepeatNum>"1, 1, 1"</PanelRepeatNum>
		<PanelDimmingGearSeq>"120, 0, 0, 0, 60, 1, 1, 1, 144, 2, 2, 2"</PanelDimmingGearSeq>
		<PanelDimmingGearLen>3</PanelDimmingGearLen>
		<PanelDimmingGearEnable>1</PanelDimmingGearEnable>
		<!--panel aod downframe dimming sequence-->
		<PanelAodDimmingSeq>"30"</PanelAodDimmingSeq>
		<!--panel aod downframe dimming sequence repeat time-->
		<PanelAodRepeatNum>"2"</PanelAodRepeatNum>
		<PanelAodDimmingGearSeq>"30, 0, 0, 0"</PanelAodDimmingGearSeq>
		<PanelAodDimmingGearLen>1</PanelAodDimmingGearLen>
		<PanelFullAodDimmingGearLen>1</PanelFullAodDimmingGearLen>
		<PanelBlTo1Hz>773</PanelBlTo1Hz>
		<PanelAodBlTo1Hz>1</PanelAodBlTo1Hz>
		<!--ltpo end-->

		<!--oneside driver mode detect test-->
		<OnesideModeSupport>0</OnesideModeSupport>
		<!--color aod detect-->
		<ColorAodDetectSupport>0</ColorAodDetectSupport>
		<!--checksum support-->
		<PanelChecksumSupport>0</PanelChecksumSupport>
		<!--vertical line test-->
		<VtcLineSupport>0</VtcLineSupport>
		<!-- Fac Check Mipi -->
		<PanelCheckMipiSupport>0</PanelCheckMipiSupport>
		<PanelCheckMipiMaxNum>3</PanelCheckMipiMaxNum>

		<!--support safe frame decision by TP-->
		<SafeFrmDecisionByTpSupport>0</SafeFrmDecisionByTpSupport>
		<!--support safe frame decision by FPS-->
		<SafeFrmDecisionByFpsSupport>1</SafeFrmDecisionByFpsSupport>
		<!--Minimum Frame Rate for fps 144 -->
		<Fps144SafeFrmRate>144</Fps144SafeFrmRate>
		<!--Minimum Frame Rate for fps 120 -->
		<Fps120SafeFrmRate>120</Fps120SafeFrmRate>
		<!-- online mipi update -->
		<TconUpdSupport>1</TconUpdSupport>

		<!--pad aod update core clk by hdm set clock rate-->
		<PadAodUpdClkSupport>1</PadAodUpdClkSupport>
		<!--update core clk support for aod-->
		<UpdateCoreClkSupport>1</UpdateCoreClkSupport>

		<!-- for dieid read  -->
		<LcdPanelDieidSupport>1</LcdPanelDieidSupport>

		<!--color calib-->
		<ColorCalibSupport>1</ColorCalibSupport>

		<!--Mipi clk_t_hs_zero adjust-->
		<MipiClkTHsZeroAdjust>4</MipiClkTHsZeroAdjust>

	</PanelEntry>
</hwlcd>
