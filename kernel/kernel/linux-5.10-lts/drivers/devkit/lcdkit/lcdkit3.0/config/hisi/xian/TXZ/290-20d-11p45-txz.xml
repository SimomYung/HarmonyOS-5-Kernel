<hwlcd>
	<PanelEntry>
		<!--/* 0:DSI0 1:DSI1 2:DSI2 */ -->
		<ConnectorId0>0</ConnectorId0>
		<!--/* Max connector number is 2 */ -->
		<ConnectorNum>1</ConnectorNum>

		<!--Ldi config start-->
		<MipiDsiTimingSupport>1</MipiDsiTimingSupport>
		<!--Mipi Horizontal sync area -->
		<MipiHSyncArea>4</MipiHSyncArea>
		<!--Mipi Horizontal back porch -->
		<MipiHBackPorch>20</MipiHBackPorch>
		<!--Mipi Horizontal line time -->
		<MipiHLineTime>605</MipiHLineTime>
		<!--Mipi Dpi horizontal size -->
		<MipiDpiHSize>402</MipiDpiHSize>
		<!--Mipi Vertical sync area -->
		<MipiVSyncArea>2</MipiVSyncArea>
		<!--Mipi Vertical back porch -->
		<MipiVBackPorch>140</MipiVBackPorch>
		<!--Mipi Vertical front porch -->
		<MipiVFrontPorch>1338</MipiVFrontPorch>
		<!--Mipi dsi default clock-->
		<MipiDsiBitClk>572</MipiDsiBitClk>
		<!--Panel information config start-->
		<!--Panel type:8-video mode, 16-command mode-->
		<PanelCmdType>8</PanelCmdType>
		<!--DSS low power setting:0-close(video mode), 7-open(command mode)-->
		<PanelVsynCtrType>0</PanelVsynCtrType>
		<!--Pixel clock-->
		<PanelPxlClk>192</PanelPxlClk>
		<!--Panel fps-->
		<PanelFps>60</PanelFps>
		<PanelTEIndex>0</PanelTEIndex>
		<!--Mipi burst mode:0-non burst sync pulse, 1-non burst sync event, 2-burst sync pulse1, 3-burst sync pulse2-->
		<MipiBurstMode>1</MipiBurstMode>
		<!--Mipi color mode:0-16bit_1, 1-16bit_2, 2-16bit_3, 3-18bit_1, 4-18bit_2, 5-24bit_1, 6-24bit_2, 7-24bit_3-->
		<MipiColorMode>15</MipiColorMode>
		<PanelPxlClkDiv>3</PanelPxlClkDiv>
		<!--Ifbc type:0-none, 1-orise2x, 2-orise3x, 3-himax2x,
		4-rsp2x, 5-rsp3x, 6-vesa2x single, 7-vesa3x single
		8-vesa2x dual, 9-vesa3x dual, 10-vesa3.75x dual-->
		<PanelIfbcType>4</PanelIfbcType>
		<VesaSliceWidth>800</VesaSliceWidth>
		<VesaSliceHeight>8</VesaSliceHeight>
		<!--0-ver1.1, 1-ver1.2-->
		<VesaDscVersion>0</VesaDscVersion>
		<!--0-RGB, 1-YUV, 2-YUV420, 3-YUV422, 4-YUV-444-->
		<VesaDscFormat>0</VesaDscFormat>
		<!--bit per pixel-->
		<VesaDscBpp>8</VesaDscBpp>
		<!--bit per component-->
		<VesaDscBpc>8</VesaDscBpc>
		<!--block pred enable-->
		<VesaDscBlkPredEn>1</VesaDscBlkPredEn>
		<!--line buff depth-->
		<VesaDscLineBuffDepth>9</VesaDscLineBuffDepth>
		<!--GENERATE_RC_PARAMETERS:0-default, 1-calculate-->
		<VesaDscGenRcParams>1</VesaDscGenRcParams>

		<!--Mipi RG_VREFSEL_EQ -->
		<MipiRgEqAdjust>3</MipiRgEqAdjust>
		<!--Mipi RG_VREFSEL_VCM -->
		<MipiRgVcmAdjust>7</MipiRgVcmAdjust>

		<DoesLcdPowerOnTp>1</DoesLcdPowerOnTp>
		<PanelOnAlwaysNeedReset>1</PanelOnAlwaysNeedReset>
		<!--Pt station test support-->
		<PanelPtTestSupport>1</PanelPtTestSupport>
		<!--check reg support-->
		<PanelCheckRegSupport>0</PanelCheckRegSupport>
		<!--Check panel power status test support-->
		<PanelCheckRegOnSupport>0</PanelCheckRegOnSupport>
		<PanelCheckRegOnSupportDsmReport>0</PanelCheckRegOnSupportDsmReport>
		<PanelCheckRegOffSupport>0</PanelCheckRegOffSupport>
		<PanelCheckRegOffSupportDsmReport>0</PanelCheckRegOffSupportDsmReport>
		<!--sn code support-->
		<PanelSnCodeSupport>1</PanelSnCodeSupport>
		<!--Oem info-->
		<OemInfoSupport>1</OemInfoSupport>
		<OemBarcode2DSupport>1</OemBarcode2DSupport>
		<!--Tp color support-->
		<TpColorSupport>1</TpColorSupport>

		<!--Esd id support-->
		<PanelEsdDoubleDetect>1</PanelEsdDoubleDetect>
		<PanelEsdSupport>1</PanelEsdSupport>
		<PanelFacEsdSupport>0</PanelFacEsdSupport>
		<EsdGpioDetectSupport>2</EsdGpioDetectSupport>
		<EsdGpioDetectNum>200</EsdGpioDetectNum>
		<EsdGpioNormalValue>0</EsdGpioNormalValue>
		<EsdGpioDetectNum1>254</EsdGpioDetectNum1>
		<EsdGpioNormalValue1>0</EsdGpioNormalValue1>
		<TpEsdGpioEvent>1</TpEsdGpioEvent>

		<!--VtcLineSupport support-->
		<VtcLineSupport>1</VtcLineSupport>

		<!--Project id support-->
		<PanelProjectIdSupport>1</PanelProjectIdSupport>
		<!--Tp Project Id-->
		<PanelDefaultTpProjectId>"W462B12700"</PanelDefaultTpProjectId>
		<!--Panel version support-->
		<PanelVersionSupport>0</PanelVersionSupport>
		<PanelVerSion>"CG,AG"</PanelVerSion>
		<PanelVerSionValue>"0xD2
				0x4B"</PanelVerSionValue>

		<!--bl type:1-none, 2-pwm, 4-blpwm, 8-mipi, 16-sh blpwm-->
		<PanelBlType>4</PanelBlType>
		<!--bl max nit-->
		<PanelBlMaxnit>600</PanelBlMaxnit>
		<!--bl min brightness setting-->
		<PanelBlmin>30</PanelBlmin>
		<!--bl max brightness setting-->
		<PanelBlmax>4095</PanelBlmax>
		<!--bl default brightness setting-->
		<PanelBlDef>597</PanelBlDef>
		<!--backlight order-->
		<BacklightOrder>0</BacklightOrder>
		<!--backlight ext byte flag-->
		<PanelBlextFlag>0</PanelBlextFlag>
		<!--backlight need prepare-->
		<BlNeedPrepare>0</BlNeedPrepare>

		<!--Mipi lane number:c-phy:0-1port, 1-2port, 2-3port d-phy:0-1lane, 1-2lane, 2-3lane, 3-4lane-->
		<MipiLaneNums>3</MipiLaneNums>
		<!--mipi dsi support-->
		<MipiDsiUptSupport>0</MipiDsiUptSupport>
		<!--Mipi phy mode:0-D-phy, 1-C-phy-->
		<MipiPhyMode>0</MipiPhyMode>
		<MipiClkPostAdjust>215</MipiClkPostAdjust>
		<MipiMaxTxEscClk>10</MipiMaxTxEscClk>
		<MipiDataTHsTrailAdjust>0xFFFFFFE4</MipiDataTHsTrailAdjust>
		<!--sensorhub aod support-->
		<SensorhubAodSupport>0</SensorhubAodSupport>
		<!--AodBlLevel:0-two level, 1-three level, 2-four level-->
		<AodBlLevel>0</AodBlLevel>
		<!--panel on diff fps-->
		<PanelFpsOnSupport>0</PanelFpsOnSupport>
		<!--lcd max nit-->
		<IMaxLcdLuminance>600</IMaxLcdLuminance>
		<!--lcd min nit-->
		<IMinLcdLuminance>3</IMinLcdLuminance>
		<!--event send
		event, data, delay
		event:0-NONE, 1-VCI, 2-IOVCC, 3-VSP, 4-VSN, 5-RESET, 6-MIPI, 7-TS early-event, 8-TS later-event, 9-vdd 10-AOD, 11-BIAS
		data:0-power off, 1-power on
		delay:ms/TS:0-no-sync 1-sync
		-->
		<!--Power on timming-->
		<PowerOnStage>"2, 1, 2
				3, 1, 2
				4, 1, 0
				11, 1, 12
				5, 2, 15
				5, 1, 10
				5, 2, 10
				5, 1, 55"</PowerOnStage>
		<LPOnStage>"7, 1, 5
			    8, 1, 0
			    6, 1, 0"</LPOnStage>
		<HSOnStage>"0, 0, 0"</HSOnStage>

		<!--Power off timming-->
		<HSOffStage>"7, 0, 0
			8, 0, 20
			6, 0, 10"</HSOffStage>
		<LPOffStage>"0, 0, 0"</LPOffStage>
		<PowerOffStage>"5, 1, 100
				5, 0, 10
				11, 0, 1
				4, 0, 5
				3, 0, 10
				2, 0, 0"</PowerOffStage>
		<!--Power timming config end-->

		<!--Backlight max nit-->
		<!--Power timming config start-->
		<!--event define
		mode, number, amplitude
		mode:0-none, 1-regulator, 2-gpio
		number:gpio num or ldo num
		amplitude:3100000-3.1v, 1850000-1.85v, 5500000-5.5v
		-->
		<IOVccRegulator>ldo13</IOVccRegulator>
		<LcdIovcc>"1, 13, 1900000"</LcdIovcc>
		<LcdVsp>"2, 194, 5900000"</LcdVsp>
		<LcdVsn>"2, 184, 5900000"</LcdVsn>
		<PowerDownVsp>"2, 194, 5900000"</PowerDownVsp>
		<PowerDownVsn>"2, 184, 5900000"</PowerDownVsn>
		<LcdReset>"2, 117, 0"</LcdReset>
		<TpReset>"2, 243, 0"</TpReset>
		<LcdMipiSwitch>"0, 0, 0"</LcdMipiSwitch>
		<LcdTe0>"0, 0, 0"</LcdTe0>
		<LcdBacklight>"0, 0, 0"</LcdBacklight>
		<!--event send-->

		<!--fps begin-->
		<PanelFpsUpdtSupport>1</PanelFpsUpdtSupport>
		<!--hsa->hbp->dpi_hsize->hline_time->vsa->vbp->vfp->mipi_clk-->
		<FpsTo60DsiTimming>"4, 20, 402, 605, 2, 140, 1338, 572"</FpsTo60DsiTimming>
		<FpsTo90DsiTimming>"4, 20, 402, 605, 2, 140, 26, 572"</FpsTo90DsiTimming>
		<FpsTo120DsiTimming>"4, 20, 402, 454, 2, 140, 26, 572"</FpsTo120DsiTimming>
		<!--oled type:0-ltps, 1-ltpo1, 2-ltpo2-->
		<OledType>0</OledType>
		<!--dfr mode:0-CONSTANT, 1-LONG_V, 2-LONG_H, 3-TE_SKIP_BY_MCU, 4-TE_SKIP_BY_ACPU-->
		<DFRMode>5</DFRMode>
		<PanelDefaultFps>60</PanelDefaultFps>
		<PanelSupportFpsList>"60 90 120"</PanelSupportFpsList>
		<!--fps end-->
	</PanelEntry>
</hwlcd>
