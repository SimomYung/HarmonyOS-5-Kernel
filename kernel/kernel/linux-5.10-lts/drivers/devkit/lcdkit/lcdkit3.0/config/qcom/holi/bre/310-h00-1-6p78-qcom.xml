<hwlcd>
	<PanelEntry>
		<!--Panel type:dsi_video_mode-video mode, dsi_cmd_mode-command mode-->
		<PanelTypeMode>"dsi_cmd_mode"</PanelTypeMode>
		<PanelModeSwitch>0</PanelModeSwitch>
		<PanelPhyWidth>70</PanelPhyWidth>
		<PanelPhyHeight>155</PanelPhyHeight>

		<!--Dsi ctrl number:1-one dsi ctrl, 2-two dsi ctrl-->
		<DsiCtrlNum>0</DsiCtrlNum>
		<!--Dsi phy number:1-one dsi phy, 2-two dsi phy-->
		<DsiPhyNum>0</DsiPhyNum>

		<!-- Lane Config -->
		<PanelDSILaneMap>"lane_map_0123"</PanelDSILaneMap>
		<PanelLane0State>1</PanelLane0State>
		<PanelLane1State>1</PanelLane1State>
		<PanelLane2State>1</PanelLane2State>
		<PanelLane3State>1</PanelLane3State>

		<!-- MIPI Host Config -->
		<!-- DsiTClkPost>0x0C</DsiTClkPost -->
		<!--DsiTClkPre>0x1A</DsiTClkPre -->
		<DsitxEotAppend>1</DsitxEotAppend>
		<DsiSelectClocks>"mux_byte_clk0", "mux_pixel_clk0","src_byte_clk0","src_pixel_clk0","shadow_byte_clk0", "shadow_pixel_clk0"</DsiSelectClocks>

		<!-- Display dsi timming config -->
		<DisplayDsiTimmingBegin>1</DisplayDsiTimmingBegin>
		<DisplayDsiTimming0Begin>1</DisplayDsiTimming0Begin>
		<DisplayDsiTimming0End>1</DisplayDsiTimming0End>
		<DisplayDsiTimming1Begin>1</DisplayDsiTimming1Begin>
		<DisplayDsiTimming1End>1</DisplayDsiTimming1End>
		<DisplayDsiTimmingEnd>1</DisplayDsiTimmingEnd>

		<!-- Display command panel config -->
		<PanelTEPinSelect>1</PanelTEPinSelect>
		<PanelWRMEMStart>0x2c</PanelWRMEMStart>
		<PanelWRMEMContinue>0x3c</PanelWRMEMContinue>
		<PanelTEDCSCmd>1</PanelTEDCSCmd>
		<PanelTECheckEnable>1</PanelTECheckEnable>
		<PanelTEUsingTEPin>1</PanelTEUsingTEPin>
		<PanelGpioAddress>0x0F100000 0x1000</PanelGpioAddress>
		<PanelGpioPins>12 2 23</PanelGpioPins>
		<PanelGpioNames>"reg-gpio", "reset-gpio", "platform-te-gpio"</PanelGpioNames>

		<!-- Display dsi timming0 config -->
		<PanelFrameRate0>60</PanelFrameRate0>
		<PanelXres0>1224</PanelXres0>
		<PanelYres0>2700</PanelYres0>
		<HFrontPorch0>20</HFrontPorch0>
		<HBackPorch0>24</HBackPorch0>
		<HPulseWidth0>4</HPulseWidth0>
		<VFrontPorch0>176</VFrontPorch0>
		<VBackPorch0>32</VBackPorch0>
		<VPulseWidth0>2</VPulseWidth0>
		<HSyncSkew0>0</HSyncSkew0>
		<PanelPhyTimings0>"00 24 0A 0A 1A 25 09
                                   0A 09 02 04 00 1E 0F"</PanelPhyTimings0>
		<DisplayTopology0>1 1 1</DisplayTopology0>
		<DefaultTopologyIndex0>0</DefaultTopologyIndex0>
		<PanelCompressionMode0>"dsc"</PanelCompressionMode0>
		<PanelDscSliceHeight0>36</PanelDscSliceHeight0>
		<PanelDscSliceWidth0>612</PanelDscSliceWidth0>
		<PanelDscSlicePerPkt0>1</PanelDscSlicePerPkt0>
		<PanelDscBitPerComponent0>8</PanelDscBitPerComponent0>
		<PanelDscBitPerPixel0>8</PanelDscBitPerPixel0>
		<PanelDscBlockPredictionEnable0>1</PanelDscBlockPredictionEnable0>

		<!-- Display dsi timming1 config -->
		<PanelFrameRate1>120</PanelFrameRate1>
		<PanelXres1>1224</PanelXres1>
		<PanelYres1>2700</PanelYres1>
		<HFrontPorch1>20</HFrontPorch1>
		<HBackPorch1>24</HBackPorch1>
		<HPulseWidth1>4</HPulseWidth1>
		<VFrontPorch1>176</VFrontPorch1>
		<VBackPorch1>32</VBackPorch1>
		<VPulseWidth1>2</VPulseWidth1>
		<HSyncSkew1>0</HSyncSkew1>
		<PanelPhyTimings1>"00 24 0A 0A 1A 19 09
                                   0A 09 02 04 00 1E 0F"</PanelPhyTimings1>
		<DisplayTopology1>1 1 1</DisplayTopology1>
				<DefaultTopologyIndex1>0</DefaultTopologyIndex1>
		<PanelCompressionMode1>"dsc"</PanelCompressionMode1>
		<PanelDscSliceHeight1>36</PanelDscSliceHeight1>
		<PanelDscSliceWidth1>612</PanelDscSliceWidth1>
		<PanelDscSlicePerPkt1>1</PanelDscSlicePerPkt1>
		<PanelDscBitPerComponent1>8</PanelDscBitPerComponent1>
		<PanelDscBitPerPixel1>8</PanelDscBitPerPixel1>
		<PanelDscBlockPredictionEnable1>1</PanelDscBlockPredictionEnable1>
		<!--Lcdkit fps support -->
		<PanelFpsUpdtSupport>1</PanelFpsUpdtSupport>
		<!--Lcdkit default fps -->
		<PanelDefaultFps>60</PanelDefaultFps>
		<!--Lcdkit fps support list -->
		<PanelSupportFpsList>"60, 120"</PanelSupportFpsList>
		<!-- DMS Config -->
		<DsiPanelDynamicModeSwitchEnabled>1</DsiPanelDynamicModeSwitchEnabled>
		<DsiPanelDynamicModeSwitchType>"dynamic-resolution-switch-immediate"</DsiPanelDynamicModeSwitchType>
		<PanelFPS60CMD>"vndservice call display.qservice 25 i32 0 i32 0"</PanelFPS60CMD>
		<PanelFPS120CMD>"vndservice call display.qservice 25 i32 1 i32 0"</PanelFPS120CMD>

		<!-- Backlight ctrl config -->
		<BlPmicControlType>"bl_ctrl_dcs"</BlPmicControlType>
		<!--backlight order-->
		<BacklightOrder>1</BacklightOrder>
		<BacklightWriteOffset>0x80</BacklightWriteOffset>
		<MdssDsiBlMinLevel>1</MdssDsiBlMinLevel>
		<MdssDsiBlMaxLevel>10000</MdssDsiBlMaxLevel>
		<DsiPanelCphyMode>0</DsiPanelCphyMode>
		<!--bl def brightness setting-->
		<PanelBldef>2047</PanelBldef>
		<PanelProjectIdSupport>1</PanelProjectIdSupport>
		<!--Tp Project Id-->
		<PanelDefaultTpProjectId>"G429922900"</PanelDefaultTpProjectId>
		<DoesLcdPowerOnTp>0</DoesLcdPowerOnTp>
		<!--OemInfo support-->
		<OemInfoSupport>1</OemInfoSupport>
		<!--OemBarcode2DSupport support-->
		<OemBarcode2DSupport>1</OemBarcode2DSupport>
		<!--OemBarcode2DNumOffset-->
		<OemBarcode2DNumOffset>0</OemBarcode2DNumOffset>
		<!--sn code support-->
		<PanelSnCodeSupport>1</PanelSnCodeSupport>
		<!--pcd errflag check support-->
		<GpioPcdErrflag>0</GpioPcdErrflag>
		<PanelPcdErrflagCheckSupport>0</PanelPcdErrflagCheckSupport>
		<PcdCmdsSupport>0</PcdCmdsSupport>
		<!--Pcd reg-value compare mode:
		0: == NG, != ok
		1: >= NG, < ok
		-->
		<PcdCmdsSupportCompareMode>0</PcdCmdsSupportCompareMode>
		<!--bl max type:0-not support, 1-ddic ytpe-->
		<PanelGetBlMaxNitType>0</PanelGetBlMaxNitType>
		<PanelGetBlMaxNitOffset>400</PanelGetBlMaxNitOffset>
		<PanelBacklightMaxNitCommand>"0x06, 0x01, 0x00, 0x00, 0x0a, 0x00, 0x01, 0xdb"</PanelBacklightMaxNitCommand>
		<PanelBacklightMaxNitCommandState>"dsi_lp_mode"</PanelBacklightMaxNitCommandState>
		<!--bl max nit-->
		<PanelBlMaxnit>450</PanelBlMaxnit>
		<!--bl min brightness setting-->
		<PanelBlmin>1</PanelBlmin>
		<!--bl max brightness setting-->
		<PanelBlmax>10000</PanelBlmax>
		<!--bl max brightness setting qcom platform-->
		<BrightMaxLevel>10000</BrightMaxLevel>
		<!--Running test config start-->
		<!--Dot/column inversion test support-->
		<PanelInversionSupport>0</PanelInversionSupport>
		<!--Check panel power status test support-->
		<FactoryCheckRegSupport>0</FactoryCheckRegSupport>
		<!--Pt station test support-->
		<PanelPtTestSupport>1</PanelPtTestSupport>
		<!--PanelOnAlwaysNeedReset-->
		<PanelOnAlwaysNeedReset>0</PanelOnAlwaysNeedReset>
		<!--gpio offset number setting-->
		<PanelGpioOffset>341</PanelGpioOffset>

		<!--Tp color support-->
		<TpColorSupport>0</TpColorSupport>
		
		<EsdEnabled>1</EsdEnabled>
		<EsdCommand>"0x29, 0x01, 0x00, 0x00, 0x00, 0x00, 0x04, 0xFF, 0x01, 0x00, 0x04
			0x06, 0x01, 0x00, 0x00, 0x00, 0x00, 0x01, 0x40
			0x29, 0x01, 0x00, 0x00, 0x00, 0x00, 0x04, 0xFF, 0x01, 0x00, 0x10
			0x06, 0x01, 0x00, 0x00, 0x00, 0x00, 0x01, 0x40
			0x29, 0x01, 0x00, 0x00, 0x00, 0x00, 0x04, 0xFF, 0x03, 0x00, 0x64
			0x06, 0x01, 0x00, 0x00, 0x00, 0x00, 0x01, 0x00
			0x29, 0x01, 0x00, 0x00, 0x00, 0x00, 0x04, 0xFF, 0x0B, 0x34, 0xC8
			0x06, 0x01, 0x00, 0x00, 0x00, 0x00, 0x04, 0xA7, 0x00, 0x00, 0x00
			0x29, 0x01, 0x00, 0x00, 0x00, 0x00, 0x04, 0xFF, 0x00, 0x00, 0x00"</EsdCommand>
		<EsdCommandState>"dsi_lp_mode"</EsdCommandState>
		<EsdCheckMode>"reg_read"</EsdCheckMode>
		<EsdReadLength>0 1 0 1 0 1 0 1 0</EsdReadLength>
		<EsdValues>"0x00","0x01","0x03","0x21"</EsdValues>
		<!--Ap aod support-->
		<ApAodSupport>1</ApAodSupport>
		<!-- AOD LP1-LP2-LP1 ,if ApAodLp2NeedSendCmd is set,LP2 to LP1 need send DSI_CMD_SET_LP1 cmd -->
		<!-- if ApAodLp2NeedSendCmd not set,LP2 to LP1 can not send DSI_CMD_SET_LP1 cmd -->
		<ApAodLp2NeedSendCmd>0</ApAodLp2NeedSendCmd>
		<!--oneside driver mode detect test-->
		<OnesideModeSupport>0</OnesideModeSupport>
		<!--color aod detect-->
		<ColorAodDetectSupport>0</ColorAodDetectSupport>
		<!--event send
		event, data, delay
		event:0-NONE, 1-VCI, 2-IOVCC, 3-VSP, 4-VSN, 5-RESET, 6-MIPI, 7-TS early event, 8-TS later event, 9-vdd 10-AOD, 11-BIAS
		data:0-power off, 1-power on
		delay:ms/TS:0-no-sync 1-sync
		-->
		<!--Power on timming-->
	<PowerOnStage>"2, 1, 1
	               9, 1, 2
		       1, 1, 10
		       5, 1, 10
		       5, 0, 10
		       5, 1, 10
		       6, 1, 1
		       7, 1, 0
		       8, 1, 0"</PowerOnStage>
		<LPOnStage>"0, 0, 0"</LPOnStage>
                 <HSOnStage>"0, 0, 0"</HSOnStage>

		<!--Power off timming-->
		<LPOffStage>"0, 0, 0"</LPOffStage>
		<PowerOffStage>"7, 0, 0
				6, 0, 1
				8, 0, 0
				5, 0, 1
				1, 0, 1
				1, 0, 1
				9, 0, 1
				2, 0, 1
				2, 0, 1"</PowerOffStage>
		<OnlyPowerOffStage>"0, 0, 0"</OnlyPowerOffStage>
		<!--Power timming config end-->
		<!--Tp color support-->
		<TpColorSupport>0</TpColorSupport>
		<!--Backlight max nit-->
		<!--Power timming config start-->
		<!--event define
		when mode is regulate to set min voltage, max voltage, enable load, disable load
		mode, number, amplitude, min voltage, max voltage, enable load, disable load
		mode:0-none, 1-regulator, 2-gpio
		number:gpio num or ldo num
		amplitude:3100000-3.1v, 1850000-1.85v, 5500000-5.5v
		min voltage:1850000-1.85v
		max voltage:5500000-5.5v
		enable load:62000
		disable load:80
		-->
		<LcdReset>"2, 2, 0"</LcdReset>
		<PlatformResetGpio>pmr735a_gpios 2 0</PlatformResetGpio>
		<DsiResetSequence>1 10 0 20 1 20</DsiResetSequence>
		<LcdIovcc>"1, 13, 1800000, 1800000, 1800000, 60700, 80"</LcdIovcc>
		<LcdVci>"1, 21, 3000000, 3000000, 3000000, 10000, 80"</LcdVci>
		<LcdVdd>"2, 107, 1200000"</LcdVdd>
		<IovccName>"vddio"</IovccName>
		<VciName>"vdd"</VciName>
		<LeftTimeThUs>28667</LeftTimeThUs>
 		<RightTimeThUs>29667</RightTimeThUs>
 		<TeInervalTimeUs>16667</TeInervalTimeUs>
 		<MaskDelayTimeBeforeFp>0</MaskDelayTimeBeforeFp>
 		<MaskDelayTimeAfterFp>16667</MaskDelayTimeAfterFp>
	</PanelEntry>
</hwlcd>
